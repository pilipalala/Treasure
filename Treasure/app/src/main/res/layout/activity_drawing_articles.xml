<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wyj.treasure.customcontrol.DrawingArticlesOneActivity">

    <include layout="@layout/title" />

    <com.wyj.treasure.customcontrol.MyDrawingViewOne
        android:id="@+id/view_one"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wyj.treasure.customcontrol.MyDrawingViewTwo
        android:id="@+id/view_two"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wyj.treasure.customcontrol.MyDrawingViewText
        android:id="@+id/view_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <com.wyj.treasure.customcontrol.MyRegionView
        android:id="@+id/view_region"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone" />

</LinearLayout>
