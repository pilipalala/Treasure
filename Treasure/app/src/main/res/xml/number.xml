<?xml version="1.0" encoding="utf-8"?><!--数字键盘number.xml-->
<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyHeight="59dip"
    android:keyWidth="33.3%p"
    android:verticalGap="6dp">
    <Row>
        <Key
            android:codes="49"
            android:horizontalGap="6dp"
            android:keyLabel="1" />
        <Key
            android:codes="50"
            android:horizontalGap="6dp"
            android:keyLabel="2" />
        <Key
            android:codes="51"
            android:horizontalGap="6dp"
            android:keyLabel="3" />
    </Row>
    <Row>
        <Key
            android:codes="52"
            android:horizontalGap="6dp"
            android:keyLabel="4" />
        <Key
            android:codes="53"
            android:horizontalGap="6dp"
            android:keyLabel="5" />
        <Key
            android:codes="54"
            android:horizontalGap="6dp"
            android:keyLabel="6" />
    </Row>
    <Row>
        <Key
            android:codes="55"
            android:horizontalGap="6dp"
            android:keyLabel="7" />
        <Key
            android:codes="56"
            android:horizontalGap="6dp"
            android:keyLabel="8" />
        <Key
            android:codes="57"
            android:horizontalGap="6dp"
            android:keyLabel="9" />
    </Row>
    <Row>
        <Key
            android:codes="-2"
            android:horizontalGap="6dp"
            android:keyLabel="切换" />
        <Key
            android:codes="48"
            android:horizontalGap="6dp"
            android:keyLabel="0" />
        <Key
            android:codes="-5"
            android:horizontalGap="6dp"
            android:keyLabel="Del" />
    </Row>
</Keyboard>